<table id="example2" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>No.</th>
            <th>Coin name</th>
            <th>Coin quantity</th>
            <th>Price</th>
            <th>Actions</th>
        </tr>
        </tr>
    </thead>
    <tbody>
        <% for(let i=0; i < nominal.length; i++) { %>
            <tr>
                <td>
                    <%= i + 1 %>
                </td>
                <td>
                    <%= nominal[i].coinName%>
                </td>
                <td>
                    <%= nominal[i].coinQuantity %>
                </td>
                <td>
                    <%= nominal[i].price %>
                </td>
                <td>
                    <form action="/nominal/delete/<%= nominal[i]._id %>?_method=DELETE" method="POST">
                        <a class=" btn btn-info btn-sm" href="/nominal/edit/<%= nominal[i]._id %>">
                            <i class="fas fa-pencil-alt"></i>
                            Update</a>
                        <button class="btn btn-danger btn-sm">
                            <i class="fas fa-trash"></i>
                            Delete</button>
                    </form>
                </td>
            </tr>
            <% } %>
    </tbody>
</table>